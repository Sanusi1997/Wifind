@{
    ViewData["Title"] = "Wi-Find";
}

<div class="jumbotron text-center">
    <h1 class="display-4">Wi-Fi Angel</h1>
    <p class="lead">A new way to share Wi-Fi</p>
    <a asp-controller="Home" asp-action="BuyerRegister" class="btn btn-primary btn-lg active rounded-2 buyerbutton" role="button" aria-pressed="true">Register/log-in as a buyer</a>
    <a asp-controller="Home" asp-action="SellerRegister" class=" btn btn-secondary btn-lg active rounded-2 sellerbutton" role="button" aria-pressed="true">Register/log-in as a seller</a>
</div>
